{
  "name": "metapprendo-rest-api",
  "version": "0.0.1",
  "description": "blockchain rest api",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "14 || 16 || 17 || 18"
  },
  "scripts": {
    "build": "lb-tsc --copy-resources",
    "build:watch": "lb-tsc --copy-resources --watch",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:unit": "npm run pretest && lb-mocha --require dist/__tests__/test-helper.js --allow-console-logs dist/**/*.test.js",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "test:e2e": "npm run pretest && cucumber-js --config cucumber.json",
    "docker:build": "docker build -t metapprendo-rest-api .",
    "docker:run": "docker run -p 3000:3000 -d metapprendo-rest-api",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "node -r source-map-support/register .",
    "dev": "DISABLE_AUTH=true npm run start",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build",
    "release": "heroku container:push --arg NPM_TOKEN=${NPM_TOKEN_SPRINSTUDIOCO} -a met-apprendo web && heroku container:release -a met-apprendo web"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Sprintstudio",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "9.0.3",
    "@loopback/authorization": "0.12.3",
    "@loopback/boot": "5.0.3",
    "@loopback/core": "4.0.3",
    "@loopback/repository": "5.0.3",
    "@loopback/rest": "12.0.3",
    "@loopback/rest-explorer": "5.0.3",
    "@loopback/service-proxy": "5.0.3",
    "basic-auth": "2.0.1",
    "fabric-ca-client": "^2.2.15",
    "fabric-network": "^2.2.15",
    "lodash": "4.17.21",
    "tslib": "2.0.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "8.5.1",
    "@loopback/build": "9.0.3",
    "@loopback/eslint-config": "13.0.3",
    "@loopback/testlab": "5.0.3",
    "@types/basic-auth": "1.1.3",
    "@types/chai": "4.3.3",
    "@types/node": "14.18.21",
    "chai": "4.3.6",
    "eslint": "8.22.0",
    "fs-monkey": "1.0.3",
    "memfs": "3.4.7",
    "sinon": "14.0.1",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  }
}
